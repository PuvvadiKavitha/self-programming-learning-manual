#include <stdio.h>



static void create_array(int N)
{
	int** a = (int **)malloc(sizeof(int *) * N);
	for (int i = 0; i < N; i++)
		a[i] = (int *)malloc(sizeof(int) * N);

	for (int i = 0; i < N; i++)
	{
		for (int j = 0; j < N; j++)
		{
			if ()
				a[i][j] = 1;
			else
				a[i][j] = 0;
		}
	}


}




